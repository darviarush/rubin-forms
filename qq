#!perl -w

#use warnings "all";
#no warnings qw/uninitialized/;

use File::Basename qw/dirname/;
use lib dirname(__FILE__)."/lib";
use Msg;

@ARGV = "qq" unless @ARGV;
my $path = file("bin/$ARGV[0].pl");
if($path) { do $path; die $@ // $! if $@ // $! }
else { msg ":empty", "Команда `", ":red", $ARGV[0], ":reset", "` не найдена"; }

1;